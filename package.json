{
  "name": "next-cloudflare",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.js\" \"src/**/*.jsx\"",
    "check-types": "tsc --noEmit",
    "compile": "pnpm run build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:staging": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --config wrangler.jsonc",
    "deploy:production": "opennextjs-cloudflare build && npx wrangler deploy --config wrangler.production.jsonc",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:local": "turso dev --db-file local.db",
    "db:push": "drizzle-kit push",
    "db:studio": "npx d1s",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "drizzle-kit seed",
    "migration:apply:local": "wrangler d1 migrations apply wilnk-staging --local",
    "migration:apply:staging": "wrangler d1 migrations apply wilnk-staging --remote --config wrangler.jsonc",
    "migration:apply:production": "wrangler d1 migrations apply wilnk-production --remote --config wrangler.production.jsonc",
    "db:setup:staging": "wrangler d1 migrations apply wilnk-staging --remote --config wrangler.jsonc",
    "db:setup:production": "wrangler d1 migrations apply wilnk-production --remote --config wrangler.production.jsonc",
    "setup:env": "./scripts/setup-env.sh"
  },
  "dependencies": {
    "@libsql/client": "^0.15.9",
    "@measured/puck": "^0.20.1",
    "@opennextjs/cloudflare": "^1.4.0",
    "@orpc/client": "^1.5.2",
    "@orpc/server": "^1.5.2",
    "@orpc/tanstack-query": "^1.5.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-form": "^1.12.3",
    "@tanstack/react-query": "^5.80.5",
    "better-auth": "^1.2.10",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color": "^5.0.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.44.2",
    "hono": "^4.8.2",
    "lucide-react": "^0.487.0",
    "motion": "^12.19.1",
    "nanoid": "^5.1.5",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "prettier": "^3.6.2",
    "radix-ui": "^1.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "2.15.4",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "zod": "^3.25.16"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.80.5",
    "@types/node": "^20.19.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "tsc-alias": "^1.8.11",
    "typescript": "^5",
    "wrangler": "^4.21.2"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
